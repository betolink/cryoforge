        "temporal": {
            "interval": [["1984-01-01T00:00:00Z", "2022-12-31T23:59:59Z"]]
        }
    },
    "summaries": {
        "eo:bands": [
            {
                "name": "velocity",
                "description": "Ice velocity measurement"
            }
        ],
        "proj:code": ["EPSG:3413", "EPSG:4326"]
    },
    "links": [
        {
            "rel": "root",
            "href": "./collection.json",
            "type": "application/json"
        }
    ]
}
